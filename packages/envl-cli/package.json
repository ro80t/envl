{
    "name": "envl-cli",
    "description": "better .env",
    "version": "0.11.2",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/ro80t/envl.git"
    },
    "license": "MIT",
    "keywords": [
        "envl",
        "envl-cli"
    ],
    "files": [
        "dist"
    ],
    "bin": {
        "envl": "./dist/envl.exe"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "lint": "eslint ./packages",
        "format": "run-p format:prettier format:toml",
        "format:prettier": "prettier . -w",
        "format:toml": "taplo format",
        "prepublishOnly": "bun run build",
        "postinstall": "node dist/postInstall.js"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.0",
        "@eslint/js": "^9.39.1",
        "@taplo/cli": "^0.7.0",
        "@types/bun": "latest",
        "bun": "^1.3.5",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-typescript": "^1.1.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "^3.7.3",
        "tsup": "^8.5.1",
        "typescript-eslint": "^8.48.0",
        "vitest": "^4.0.15"
    },
    "dependencies": {
        "@types/yauzl": "^2.10.3",
        "yauzl": "^3.2.0"
    }
}
